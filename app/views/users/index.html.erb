<section class="section">
  <div class="container">
    <form action="/searchuser" role="search" method="get">
      <div class="field is-grouped is-centered">
        <div class="control is-expanded">
          <input class="input is-2" type="text" name="q" placeholder="Search">
        </div>
        <div class="control">
          <button class="button is-info" type="submit">
            Search
          </button>
        </div>
      </div>
    </form>
  </div>
</section>

<div id="notes">
  <% @users.each do |user| %>
    <% if user.nickname != "superadmin" %>
      <%= render user %>
    <% end %>
  <% end %>
</div>